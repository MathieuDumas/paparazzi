    <block name="bloc 1">
	<set value="8000" var="radio_control.values[0]"/>
	<set value="-4000" var="radio_control.values[1]"/>
	<set value="0" var="radio_control.values[2]"/>
	<set value="4000" var="radio_control.values[3]"/>
        <while cond="LessThan(NavBlockTime(), 10)"/>
    </block>

    <block name="bloc 2">
	<set value="5000" var="radio_control.values[0]"/>
	<set value="9500" var="radio_control.values[1]"/>
	<set value="-9500" var="radio_control.values[2]"/>
	<set value="-3300" var="radio_control.values[3]"/>
        <while cond="LessThan(NavBlockTime(), 10)"/>
    </block>

    <block name="bloc 3">
	<set value="1700" var="radio_control.values[0]"/>
	<set value="1500" var="radio_control.values[1]"/>
	<set value="1700" var="radio_control.values[2]"/>
	<set value="1300" var="radio_control.values[3]"/>
        <while cond="LessThan(NavBlockTime(), 10)"/>
    </block>

    <block name="Fin de test" strip_button="Standby" strip_icon="home.png">
      	<set value="1500" var="radio_control.values[0]"/>
	<set value="1500" var="radio_control.values[1]"/>
	<set value="1500" var="radio_control.values[2]"/>
	<set value="1500" var="radio_control.values[3]"/>
