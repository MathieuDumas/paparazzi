  <exceptions>
    <exception cond="RcChannel(RADIO_GEAR) > 6500" deroute="KILL"/>
    <exception cond="radio_control.time_since_last_frame > 10800" deroute="KILL"/>
    <exception cond="datalink_time > 30 && 180 > datalink_time && GpsFixValid() &&  6500 > RcChannel(RADIO_GEAR) && radio_control.status == RC_OK" deroute="Standby"/>
    <exception cond="datalink_time > 180" deroute="KILL"/>
    <exception cond="!GpsFixValid() && radio_control.status == RC_REALLY_LOST" deroute="KILL"/>
  </exceptions>
  <blocks>
    <block name="Wait GPS">
      <set value="1" var="kill_throttle"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Holding point">
      <set value="1" var="kill_throttle"/>
      <attitude pitch="0" roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block name="Takeoff 100%" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="estimator_z > ground_alt + 30" deroute="Standby"/>
      <set value="0" var="kill_throttle"/>
      <set value="0" var="combi_switch_status"/>
      <set value="8100" var="takeoff_gyro_value"/>
      <attitude pitch="30.0" roll="0.0" throttle="1.0" vmode="throttle"/>
    </block>
    <block key="s" name="Standby" strip_button="Standby" strip_icon="home.png">
      <set value="FALSE" var="h_ctl_disabled"/>
      <set value="-1000" var="takeoff_gyro_value"/>
      <set value="1" var="combi_switch_status"/>
      <set value="0" var="kill_throttle"/>
      <circle radius="nav_radius" wp="STDBY"/>
    </block>
    <block name="MOB" strip_button="Turn around here" strip_icon="mob.png">
      <call fun="NavSetWaypointHere(WP_MOB)"/>
      <circle radius="nav_radius" wp="MOB"/>
    </block>
    <block name="Oval 1-2" strip_button="Oval (wp 1-2)" strip_icon="oval.png">
      <oval p1="O_1" p2="O_2" radius="nav_radius"/>
    </block>
    <block group="mission" name="Route" strip_button="Route">
      <set value="5" var="route_ap_time"/>
      <go approaching_time="route_ap_time" wp="W1"/>
      <go approaching_time="route_ap_time" from="W1" hmode="route" vmode="glide" wp="W2"/>
      <go approaching_time="route_ap_time" from="W2" hmode="route" vmode="glide" wp="W3"/>
      <go approaching_time="route_ap_time" from="W3" hmode="route" vmode="glide" wp="W4"/>
      <go approaching_time="route_ap_time" from="W4" hmode="route" vmode="glide" wp="W5"/>
      <go approaching_time="route_ap_time" from="W5" hmode="route" vmode="glide" wp="W6"/>
      <go approaching_time="route_ap_time" from="W6" hmode="route" vmode="glide" wp="W7"/>
      <go approaching_time="route_ap_time" from="W7" hmode="route" vmode="glide" wp="W8"/>
      <go approaching_time="route_ap_time" from="W8" hmode="route" vmode="glide" wp="W9"/>
      <deroute block="Standby"/>
    </block>
    <block group="mission" name="Search" strip_button="Search">
      <set value="5" var="route_ap_time"/>
      <go approaching_time="route_ap_time" wp="SP_0"/>
      <go approaching_time="route_ap_time" from="SP_0" hmode="route" vmode="glide" wp="SP_1"/>
      <go approaching_time="route_ap_time" from="SP_1" hmode="route" vmode="glide" wp="SP_2"/>
      <go approaching_time="route_ap_time" from="SP_2" hmode="route" vmode="glide" wp="SP_3"/>
      <go approaching_time="route_ap_time" from="SP_3" hmode="route" vmode="glide" wp="SP_4"/>
      <go approaching_time="route_ap_time" from="SP_4" hmode="route" vmode="glide" wp="SP_5"/>
      <go approaching_time="route_ap_time" from="SP_5" hmode="route" vmode="glide" wp="SP_6"/>
      <go approaching_time="route_ap_time" from="SP_6" hmode="route" vmode="glide" wp="SP_7"/>
      <go approaching_time="route_ap_time" from="SP_7" hmode="route" vmode="glide" wp="SP_8"/>
      <go approaching_time="route_ap_time" from="SP_8" hmode="route" vmode="glide" wp="SP_9"/>
      <go approaching_time="route_ap_time" from="SP_9" hmode="route" vmode="glide" wp="SP_10"/>
      <go approaching_time="route_ap_time" from="SP_10" hmode="route" vmode="glide" wp="SP_11"/>
      <go approaching_time="route_ap_time" from="SP_11" hmode="route" vmode="glide" wp="SP_12"/>
      <deroute block="Standby"/>
    </block>
    <block group="mission" name="SRIC" strip_button="SRIC">
      <circle radius="nav_radius" wp="SRIC_C"/>
    </block>
    <block group="landing" name="Landing route" strip_button="Land">
      <go wp="L1"/>
      <go approaching_time="route_ap_time" from="L1" hmode="route" vmode="glide" wp="L2"/>
      <go approaching_time="route_ap_time" from="L2" hmode="route" vmode="glide" wp="L3"/>
      <deroute block="Land"/>
    </block>
    <block name="Land">
      <go approaching_time="1" from="L3" hmode="route" vmode="glide" wp="AF"/>
      <deroute block="Final"/>
    </block>
    <block key="F" name="Final">
      <set value="1" var="kill_throttle"/>
      <attitude pitch="0" roll="0" throttle="0.0" vmode="throttle"/>
      <while cond="TRUE"/>
    </block>
    <block group="landing" key="A" name="Abort" strip_button="Abort">
      <set value="0" var="kill_throttle"/>
      <attitude pitch="30" roll="0" throttle="1.0" until="stage_time>5" vmode="throttle"/>
      <deroute block="Standby"/>
    </block>
    <block group="Failsafe" name="KILL" strip_button="Kill">
      <while cond="TRUE">
        <set value="1" var="kill_throttle"/>
        <set value="TRUE" var="h_ctl_disabled"/>
        <set value="(-MAX_PPRZ)" var="h_ctl_aileron_setpoint"/>
        <set value="(MAX_PPRZ)" var="h_ctl_elevator_setpoint"/>
        <set value="MAX_PPRZ" var="flaps_setpoint"/>
      </while>
    </block>
    <block group="Failsafe" name="Save" strip_button="Save">
      <set value="1" var="kill_throttle"/>
      <set value="FALSE" var="h_ctl_disabled"/>
      <set value="0" var="flaps_setpoint"/>
      <deroute block="Holding point"/>
    </block>
  </blocks>
</flight_plan>
