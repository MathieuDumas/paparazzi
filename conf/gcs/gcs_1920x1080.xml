<layout width="1920" height="1080">
  <columns>
    <rows SIZE="450">
      <widget SIZE="300" NAME="strips"/>
      <widget NAME="aircraft"/>
    </rows>
    <rows>
      <widget SIZE="700" NAME="map2d">
        <papget type="expression" display="text" x="1290" y="592">
          <property name="scale" value="1"/>
          <property name="expr" value="(ESTIMATOR.z*3.280840)"/>
          <property name="format" value="Alt:%.1fft"/>
          <property name="size" value="17."/>
          <property name="color" value="green"/>
        </papget>
      </widget>
      <columns>
        <rows>
          <widget SIZE="150" NAME="altgraph"/>
          <widget NAME="alarms"/>
        </rows>
      </columns>
    </rows>
  </columns>
</layout>
